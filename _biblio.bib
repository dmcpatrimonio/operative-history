@article{frajndlich:2016dois23,
  title = {Dois projetos: os anos de formação de Manfredo Tafuri},
  shorttitle = {Dois projetos},
  author = {Frajndlich, Rafael Urano},
  date = {2016-07-04},
  journaltitle = {Pós. Revista do Programa de Pós-Graduação em Arquitetura e Urbanismo da FAUUSP},
  shortjournal = {Pós.},
  volume = {23},
  pages = {72},
  issn = {2317-2762, 1518-9554},
  doi = {10.11606/issn.2317-2762.v23i39p72-89},
  abstract = {Diante de sua ampla contribuição como historiador, é fácil esquecer que Manfredo Tafuri (1935-1994) teve uma breve atividade como arquiteto no primeiro quinquênio dos anos 60. Durante o período, o então jovem intelectual conciliou teoria e prática, algo impensável posteriormente, até que uma sucessão de conflitos políticos o levou à renúncia, não só do projeto, mas de um primeiro arcabouço histórico. Longe de ter sido uma atividade auxiliar em seus estudos, a prática projetual foi para o jovem Tafuri uma frente política de transformação, câmara de decantação de seus estudos de história e filosofia, no qual testavam-se diversas hipóteses de suas premissas historiográficas ainda em constituição. Parte-se de suas primeiras publicações como estudante em Roma, em 1960, até o livro Teorias e História da Arquitetura, de 1968, em que o autor rompe definitivamente com a noção de arquiteto historiador. Objetiva-se com este artigo captar a transformação teórica pela qual passa a obra do autor nos anos 60, procurando revelar um movimento entre dois projetos políticos e duas interpretações dos usos da história na arquitetura, revisitando os desenhos e pesquisas iniciais da carreira de Tafuri, e como eles ecoaram em sua trajetória posterior.},
